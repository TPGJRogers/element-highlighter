{
  "name": "highlight-selector",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:_core": "web-ext run --no-config-discovery --source-dir extension --start-url https://github.com/matatk/highlight-selector",
    "start:chrome": "npm run start:_core -- --target chromium",
    "start:firefox": "npm run start:_core -- --target firefox-desktop",
    "test": "eslint ."
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.0",
    "eslint": "^8.23.0",
    "release-it": "^15.4.1"
  },
  "overrides": {
    "conventional-changelog-conventionalcommits": "^5.0.0"
  }
}
