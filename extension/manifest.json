{
  "author": "Matthew Tylee Atkinson",
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
    "default_title": "Highlight Selector"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "highlight-selector@matatk.agrip.org.uk"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+H"
      }
    }
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Highlight (and optionally landmark) elements on the page that match a selector",
  "homepage_url": "http://github.com/matatk/highlight-selector",
  "manifest_version": 2,
  "name": "Highlight Selector",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "short_name": "Highlight Selector",
  "version": "0.1.0"
}
